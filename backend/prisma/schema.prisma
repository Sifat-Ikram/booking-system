generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Booking {
  id          Int      @id @default(autoincrement())
  resource    String
  startTime   DateTime
  endTime     DateTime
  requestedBy String
  createdAt   DateTime @default(now())
}
